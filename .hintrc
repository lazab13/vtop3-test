{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/forms": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "manifest-app-name": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          " is not supported by Chrome, Chrome Android, Edge, Firefox, Firefox for Android, Opera, Safari, Safari on iOS, Samsung Internet. Add "
        ]
      }
    ]
  }
}